{
  "name": "@jalexw/calendar-ics-parser",
  "version": "0.0.7",
  "private": false,
  "repository": "https://github.com/jalexw/calendar-ics-parser.git",
  "license": "MIT",
  "scripts": {
    "cli": "bun run src/cli.ts",
    "test": "bun test",
    "build:cjs": "bun build ./src/index.ts --outfile ./dist-cjs/index.cjs --format cjs --target node",
    "build:esm": "bun run ./build-esm.ts",
    "build:types": "tsc --project tsconfig.json",
    "build": "bun run build:types && bun run build:cjs && bun run build:esm"
  },
  "dependencies": {
    "commander": "14.0.1",
    "zod": "4.1.12"
  },
  "packageManager": "bun@1.3.1",
  "devDependencies": {
    "typescript": "5.9.3",
    "bun-types": "1.3.1",
    "tsc-alias": "1.8.16",
    "esbuild": "0.27.0"
  },
  "type": "module",
  "module": "dist-esm/index.js",
  "main": "dist-cjs/index.cjs",
  "types": "dist-types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "@jalexw/calendar-ics-parser": "run-built-cli.cjs"
  },
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist-esm/index.js",
      "require": "./dist-cjs/index.cjs"
    },
    "./*": {
      "types": "./dist-types/*",
      "import": "./dist-esm/*",
      "require": "./dist-cjs/*"
    }
  }
}
